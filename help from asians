javascript:function changeGrades() {x=document.getElementsByClassName("mark"); y=document.getElementsByClassName("score"); for(var i = 0; i < x.length; i++){if((Math.floor(Math.random()*2))==0){ x[i].innerText="A+"; y[i].innerText=(((Math.round(10*(98+(Math.random()*2))))/10))+"%";} else{x[i].innerText="A"; y[i].innerText=(((Math.round(10*(96+(Math.random()*2))))/10))+"%";} }} changeGrades();
