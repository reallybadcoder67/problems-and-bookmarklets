javascript:var cookies = 0; var increaseamount = 1; var cursorLevel = 1; var cursorPrice = 100; var i = 0; while(true){ if(i==0){ if(confirm("cookie clicker v2.1 \ncookies: "+cookies+"\ncursor level: "+cursorLevel+"\ncursor upgrade price: "+cursorPrice+"\nOk=click, Cancel=Upgrade, Cancel when not enough cookies=close cookie clicker")){ cookies+=increaseamount; i--; } else { if(cookies>=cursorPrice){ cookies-=cursorPrice; cursorPrice+=150; cursorLevel++; increaseamount++; i--; } else {break;}} i++; } }
