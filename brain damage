javascript:function randomCase(value){x = 0;tmp="";for(var i = 0;i<value.length;i++){x=Math.floor(Math.random()*2);if(x==0){tmp+=value.charAt(i).toUpperCase();}else{tmp+=value.charAt(i).toLowerCase();}}return tmp;}arr = document.getElementsByTagName("*");for(var i = 0;i<arr.length;i++) {arr[i].style.fontFamily="Comic Sans MS";if(arr[i].childElementCount>0||arr[i].innerHTML===""){continue;}arr[i].textContent= String(randomCase(arr[i].textContent)); }undefined;
